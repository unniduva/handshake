{
  "name": "handshake",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://handshake-in.web.app/",
  "main": "",
  "dependencies": {
    "@firebase/storage": "^0.3.26",
    "@rematch/core": "^1.3.0",
    "antd": "^3.26.4",
    "axios": "^0.19.2",
    "firebase": "^7.8.1",
    "inspector": "^0.5.0",
    "js-cookie": "^2.2.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.12.0",
    "react-final-form": "^6.4.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-slick": "^0.26.0",
    "reactjs-localstorage": "^0.0.8",
    "recompose": "^0.30.0",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "start": "PORT=4000 REACT_APP_ENV=development react-app-rewired start",
    "sass:watch": "node-sass -w src/assets/scss/style.scss -o src/assets/css",
    "sass:build": "node-sass src/assets/scss/style.scss -o src/assets/css --source-map true",
    "design": "PORT=4000 react-app-rewired start",
    "dev": "npm-run-all --parallel design sass:watch",
    "start:stag": "react-app-rewired start",
    "test:unit": "react-app-rewired test --env=jsdom",
    "lint": "eslint src/app/. --config .eslintrc.json",
    "build": "GENERATE_SOURCEMAP=false REACT_APP_ENV=development react-app-rewired build ",
    "stag": "GENERATE_SOURCEMAP=false REACT_APP_ENV=staging react-app-rewired build  && firebase deploy --only hosting",
    "prod": "GENERATE_SOURCEMAP=false REACT_APP_ENV=production react-app-rewired build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "eject": "react-app-rewired eject",
    "clean": "./node_modules/.bin/rimraf tmp && mkdir tmp && mkdir tmp/reports && mkdir tmp/screenshots",
    "test:firefox": "cross-env NIGHTWATCH_ENV=firefox npm test",
    "reports": "yarn run reports:junit && node cucumber-conf.js",
    "reports:junit": "cat tmp/reports/cucumber.json | cucumber-junit > tmp/reports/cucumber.junit.xml",
    "test": "yarn run clean && cucumber-js --require-module babel-core/register --require cucumber-conf.js --format node_modules/cucumber-pretty --format json:tmp/reports/cucumber_report.json --require src/test/step-definitions  -- src/test/features/**/*.feature"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chromedriver": "^79.0.0",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.1.0",
    "cucumber-junit": "^1.7.1",
    "cucumber-pretty": "^6.0.0",
    "customize-cra": "^0.9.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-plugin-mocha": "^6.2.2",
    "express": "^4.17.1",
    "flow-bin": "^0.114.0",
    "geckodriver": "^1.19.1",
    "gensync": "^1.0.0-beta.1",
    "nightwatch": "^1.3.2",
    "nightwatch-api": "^3.0.1",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "react-scripts": "^3.3.0",
    "react-test-renderer": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "saucelabs": "^3.0.1",
    "selenium-server": "^3.141.59",
    "source-map-explorer": "^2.1.2",
    "workbox-build": "^4.3.1"
  },
  "jest": {},
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
